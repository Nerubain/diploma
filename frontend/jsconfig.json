{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es6",
    "module": "es6",
    "allowSyntheticDefaultImports":true,
    "jsx": "react",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@context/*": ["src/context/*"],
      "@store/*": ["src/store/*"],
      "@hooks/*": ["src/hooks/*"],
      "@functions/*":["src/assets/functions/*"]
    }
  },
  "exclude": ["node_modules"]
}