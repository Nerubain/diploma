{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "paths": {
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"],
      "@context/*": ["context/*"],
      "@store/*": ["store/*"],
      "@hooks/*": ["hooks/*"],
      "@functions/*": ["utils/functions/*"]
    }
  },
  "exclude": ["node_modules"]
}
